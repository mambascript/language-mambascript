{
  "name": "language-mambascript",
  "displayName": "language-mambascript",
  "description": "Language Support For MambaScript",
  "version": "0.0.4",
  "main": "index.js",
  "author": {
    "name": "Arthur Bernier Jr",
    "email": "ma2bernier@gmail.com"
  },
  "license": "MIT",
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "mamba",
      "aliases": ["MambaScript", "mamba"],
      "extensions": [".mamba",".kofu"],
      "configuration": "./language-configuration.json",
      "icon": {
        "light": "./snippets/icon.png",
        "dark": "./snippets/icon.png"
      }
    }],
    "grammars": [{
      "language": "mamba",
      "scopeName": "source.mamba",
      "path": "./syntaxes/mamba.tmLanguage.json"
    }],
    "snippets": [
			{
				"language": "mamba",
				"path": "./snippets/language-mambascript.json"
			}
		]
  }
}
